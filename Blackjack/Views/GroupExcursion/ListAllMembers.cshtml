
@{
    ViewBag.Title = "ListAllMembers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<br />
<a href="/GroupExcursion/AddGroupMember">Add a new Member</a>
<br />


<br />
<br />
<h2>Customer List</h2>
<table border="1">
    <tr><th>First Name</th><th>Last Name</th><th>Email</th><th>ExcursionID</th></tr>
    @foreach (var element in ViewBag.MemberList)
    {
        <tr>
            <td>@element.FirstName</td>
            <td>@element.LastName</td>
            <td>@element.Email</td>
            <td>@element.ExcursionID</td>
        </tr>
    }
</table>

